	</javaModelGenerator>
	<sqlMapGenerator
		targetPackage="com.abcproduct.domain.repository"
		targetProject="AbcProduct/src/main/resources">
		<property name="enableSubPackages" value="true" />
	</sqlMapGenerator>
	<javaClientGenerator
		targetPackage="com.abcproduct.domain.repository"
